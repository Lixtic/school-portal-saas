{
    "version": 2,
    "builds": [
        {
            "src": "school_system/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "school_system/wsgi.py"
        }
    ],
    "buildCommand": "bash build_files.sh",
    "env": {
        "PYTHON_VERSION": "3.12"
    }
}